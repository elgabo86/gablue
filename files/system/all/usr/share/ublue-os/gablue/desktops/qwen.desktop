[Desktop Entry]
Name=Qwen
Exec=konsole --workdir /tmp -e bash -c "mkdir -p /tmp/qwen && if [ -n '%f' ] && [ -d '%f' ]; then cd '%f'; else cd /tmp/qwen; fi && /home/linuxbrew/.linuxbrew/bin/qwen; read -p 'Qwen s''est terminé, appuyez sur Entrée pour continuer...'"
Type=Application
Terminal=false
MimeType=inode/directory;
Icon=utilities-terminal
Categories=Utility;Core;Qt;
StartupNotify=false
NoDisplay=false
