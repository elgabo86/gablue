[Desktop Entry]
Name=Qwen
Exec=bash -i -c "mkdir -p /tmp/qwen && if [ -n '%f' ] && [ -d '%f' ]; then cd '%f'; else cd /tmp/qwen; fi && qwen || true; echo 'Appuyez sur Entrée pour fermer la fenêtre ou Ctrl+C pour quitter immédiatement...'; trap 'exit 0' INT; read -n 1"
Type=Application
Terminal=true
MimeType=inode/directory;
Icon=utilities-terminal
Categories=Utility;Core;Qt;
StartupNotify=false
NoDisplay=false
